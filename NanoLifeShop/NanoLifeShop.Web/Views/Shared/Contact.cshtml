
<div class="contact" id="contact">
    <div class="col-md-7 contact_left">
        <div class="grid_3">
            <ul class="msg">
                <i class="msg_icon"> </i>
                <li class="msg_desc">
                    Lorem ipsum dolor sit amet, consectetur adipádfasdfasdfus.
                </li>
            </ul>
        </div>
        <div class="grid_4">
            <h4></h4>
        </div>
        <div class="grid_5">
            <!--Show google Map-->
            <div class="map-canvas" id="map">

            </div>
        </div>
    </div>
